### General system files ###
.DS_Store                  # macOS folder attributes
Thumbs.db                  # Windows thumbnail cache
*.log                      # Log files
*.tmp                      # Temporary files
*.bak                      # Backup files

### Python (FastAPI backend) ###
# Bytecode and cache
__pycache__/               # Python bytecode cache
*.py[cod]                  # Compiled Python files
*$py.class                 # Jython compiled files
*.pyc                      # Python compiled files
*.pyo                      # Python optimized bytecode
*.pyd                      # Python dynamic modules
.cpython-*.pyc             # CPython-specific bytecode

# Virtual environments
.venv/                     # Python virtual environment
venv/                      # Alternative venv folder
env/                       # Another common venv name
ENV/                       # Sometimes used venv name
.env/                      # Environment configuration
.env.local                 # Local environment overrides

# Dependencies and builds
*.egg-info/                # Python package metadata
*.egg                      # Python package
/dist/                     # Built distribution
/build/                    # Build artifacts
/pip-wheel-metadata/       # Pip wheel metadata

# Testing and coverage
.coverage                  # Coverage data
htmlcov/                   # HTML coverage reports
.pytest_cache/             # Pytest cache
.tox/                      # Tox environments
.nox/                      # Nox environments
test-reports/              # Test reports

### Frontend (Node.js/React/Vue) ###
# Dependencies
node_modules/              # Node.js modules
.npm                       # npm config
.pnpm                      # pnpm files
.yarn-integrity            # Yarn integrity file
.yarn/cache                # Yarn cache
.yarn/unplugged            # Yarn unplugged packages
.yarn/build-state.yml      # Yarn build state
.yarn/install-state.gz     # Yarn install state

# Build artifacts
dist/                      # Production build
build/                     # Common build folder
.next/                     # Next.js build
out/                       # Alternative build folder
.serverless/               # Serverless deployments
.serverless_nextjs/        # Next.js serverless
.storybook-out/            # Storybook output

# Cache folders
.cache/                    # General cache
.turbo/                    # Turborepo cache
.eslintcache               # ESLint cache

### IDE (VS Code) ###
.vscode/*                  # All VSCode files
!.vscode/settings.json     # Except settings
!.vscode/tasks.json        # Except tasks
!.vscode/launch.json       # Except launch configs
!.vscode/extensions.json   # Except extensions
.idea/                     # JetBrains IDE config

### Environment variables ###
.env                       # Environment file
.env.development           # Dev environment
.env.production            # Prod environment
.env.test                  # Test environment
.env.local                 # Local overrides

### Docker ###
docker-compose.override.yml # Docker override file

### Databases ###
*.db                       # Database files
*.sqlite                   # SQLite databases
*.db-journal               # SQLite journals
db.sqlite3                 # Common SQLite name

### Logs ###
logs/                      # Log directory
*.log                      # Log files
npm-debug.log*             # npm debug logs
yarn-debug.log*            # Yarn debug logs
yarn-error.log*            # Yarn error logs

### Miscellaneous ###
/coverage/                 # Coverage reports
/.nyc_output/              # NYC coverage output
/.sass-cache/              # Sass cache
